#include <stdio.h>
struct student{
    int rollno;
    float cgpa;
    char name[100];
};


int main() {
    struct student s1;
    s1.rollno=1664;
    s1.cgpa=9.4;
    strcpy(s1.name,"harsh");
    printf("student name=%s\n",s1.name);
    printf("student rollno=%d\n",s1.rollno);
    printf("student cgpa=%f\n",s1.cgpa);
    
    
    
    return 0;
}
